@model NaproKarta.ViewModels.ObservationEditVM

<div class="groupBox">
    <table class="propertiesHeight">
        @foreach (var item in Model.CommentsVM)
        {
        <tr>
            <td style="text-align: left; padding-right: 15px">
                <label style="padding: 1px; margin: 1px">
                    @Html.CheckBoxFor(m => item.Checked, new { @checked = "checked" })
                    @*&nbsp; @Html.DisplayNameFor(model => item.Content)*@
                    &nbsp; @Html.DisplayName(item.Content)
                </label>
            </td>
        </tr>
        }
    </table>
</div>