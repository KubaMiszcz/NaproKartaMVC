@model NaproKarta.ViewModels.ObservationEditVM

<div class="groupBox">
    <table class="propertiesHeight">
        <tr>
            @foreach (var col in Model.CiphersCDTwoColumns)
            {
            <td style="padding: 5px; vertical-align: top;">
                <table>
                    @foreach (var item in col)
                    {
                    <tr>

                        <td style="text-align: left; padding-right: 15px">
                            <div id=@item onclick="ColorOnCheck(this.id)" class="runchecked" >
                                <label style="padding: 1px; margin: 1px" >
                                    @Html.RadioButtonFor(x => x.Observation.CipherCD.Value, item, new { @style = "color: red; background: red" })
                                    @item
                                </label>
                            </div>
                        </td>
                    </tr>
                    }
                </table>
            </td>
            }
        </tr>
    </table>
</div>